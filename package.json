    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.32.4",
    "jsdom": "^22.1.0",
    "papaparse": "^5.4.1",
    "prettier": "^3.0.2",
